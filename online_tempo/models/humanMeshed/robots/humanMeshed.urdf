<?xml version="1.0"?>
<robot name="humanMeshed">

  <link name="base_link">
    <visual>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
    </visual>
  </link>

<!--   HIP starts here  -->
  <link name="hip_roll"/>

