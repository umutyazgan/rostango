    <origin xyz="0 0 0"/>
    <axis xyz="0 1.0 0"/>
  </joint>

  <link name="hip_yaw">
        <visual>
      <geometry>  
        <cylinder length="0.352" radius="0.05"/>
     </geometry>
      <origin rpy="1.57075 0 0" xyz="0 0 0" />
    </visual>
  </link>

  <joint name="hip_yaw" type="continuous">
    <parent link="hip_pitch"/>
    <child link="hip_yaw"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 0 1.0"/>
  </joint>

